{
  "title": "My title from book.json",
  "pubdate": "2023-04-24",
  "root": "./",
  
  "plugins": [
    "hints",
    "anchorjs"
  ],
  
  "pluginsConfig": {
    "anchorjs": {
      "selector": "h1, h2, h3, h4, h5",
      "x-icon": "ðŸ‘„",
